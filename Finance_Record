CREATE TABLE "F4RAHIM"."FINCANCE_RECORD" 
   (	"HEALTH_CARD_NUMBER" NUMBER NOT NULL ENABLE, 
	"PAYMENT_PROGRAM" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"PRIVATE_INSURANCE" CHAR(100 BYTE) NOT NULL ENABLE, 
	"ADMISSION_DATE" DATE NOT NULL ENABLE, 
	"DISCHARGE_DATE" DATE NOT NULL ENABLE, 
	"STATUS_CHARGE" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"ACCESSED_AMOUNT" VARCHAR2(10 BYTE) NOT NULL ENABLE, 
	"BILLING_ADDRESS" CHAR(100 BYTE) NOT NULL ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "DBCOURSE" ;
