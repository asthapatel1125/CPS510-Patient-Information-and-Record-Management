
  CREATE TABLE "A337PATE"."MEDICINE_RECORD" 
   (	"HEALTH_ID"          NUMBER(*,0)        NOT NULL ENABLE, 
	    "START_DATE"         DATE               NOT NULL ENABLE, 
	    "DOSAGE"             NUMBER(3,1)        NOT NULL ENABLE, 
	    "MEDICINE"           VARCHAR2(200 BYTE) NOT NULL ENABLE, 
	    "SIDE_EFFECTS_NOTES" VARCHAR2(200 BYTE) NOT NULL ENABLE, 
	
  FOREIGN KEY ("HEALTH_ID")
	  REFERENCES "A337PATE"."MEDICAL_RECORD" ("HEALTH_CARD_NUMBER") ENABLE
   ) SEGMENT CREATION DEFERRED 
