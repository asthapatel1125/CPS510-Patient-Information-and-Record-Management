
  CREATE TABLE "A337PATE"."GENERAL_MEDICAL_RECORD" 
   (	"HEALTH_ID"        NUMBER             NOT NULL ENABLE, 
	    "BLOOD_TYPE"       VARCHAR2(2 BYTE)   NOT NULL ENABLE, 
	    "WEIGHT"           NUMBER(3,1)        NOT NULL ENABLE, 
	    "BLOOD_PRESSURE"   NUMBER(3,0)        NOT NULL ENABLE, 
	    "TEMPERATURE"      NUMBER(3,1)        NOT NULL ENABLE, 
	    "HEIGHT"           NUMBER(3,0)        NOT NULL ENABLE, 
	    "ALLERGY"          VARCHAR2(100 BYTE) NOT NULL ENABLE, 
	    "ALLERGY_REACTION" VARCHAR2(500 BYTE) NOT NULL ENABLE, 
      
	 FOREIGN KEY ("HEALTH_ID")
	  REFERENCES "A337PATE"."MEDICAL_RECORD" ("HEALTH_CARD_NUMBER") ENABLE
    
   ) SEGMENT CREATION DEFERRED 
