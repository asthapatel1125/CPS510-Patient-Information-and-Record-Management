CREATE TABLE "F4RAHIM"."SURGERY_RECORD" 
   (	"LAB_RESULTS" VARCHAR2(1 BYTE) NOT NULL ENABLE, 
	"MEDICAL_SCANS" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"REASON_FOR_SURGERY" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"PRESCRIBED_MEDICATION" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"POST_SURGERY" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"RECOVERY" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"DATE_OF_SURGERY" DATE NOT NULL ENABLE, 
	"SURGEON" VARCHAR2(40 BYTE) NOT NULL ENABLE, 
	"SURGEON_CLINICAL_ID" NUMBER NOT NULL ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "DBCOURSE" ;
